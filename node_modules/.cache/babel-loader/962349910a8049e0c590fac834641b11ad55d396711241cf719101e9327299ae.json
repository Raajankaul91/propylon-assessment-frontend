leVersion){\n                    return file['location']==fileURL && fileName==file['file_name'];\n                }else{\n                    return fileName==file['file_name'] && parseInt(fileVersion)==file['version_number'] && file['location']==fileURL;\n                }                \n            });\n        },\n\n        cancelSearch: function(){\n            this.searchValue = \"\";\n            this.files = this.filteredFile;\n            \n        },\n\n        DisplayFile: function(file){\n            this.displayFile = file;\n            \n        }\n    }\n\n}\n</script>\n\n<style scoped>\n.center-screen{\n  width: 500px;\n  height: 300px;\n  margin: auto;\n  margin-top: 40px;\n  padding: 10px;\n}\n.left-margin{\n  margin-left: 50px;\n}\n\n.square{\n    width:800px;\n    height: 500px;\n}\n\n.search-bar{\n    width: 600px;\n    margin-top: 20px\n}\n</style>"],"mappings":"AAwEA,OAAO,KAAKA,OAAM,MAAO,iBAAiB;AAE1C,eAAe;EACXC,IAAI,EAAC,aAAa;EAClBC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAC,CAAC,CAAC;MACRC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAC,CAAC,CAAC;MACfC,WAAW,EAAC,CAAC;IACjB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,QAAQ,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAC;IACJD,QAAQ,EAAE,SAAAA,CAAA,EAAU;MAChBT,OAAO,CAACS,QAAQ,CAAC,CAACE,KAAK,EAAEC,QAAQ,KAAK;QAClC,IAAGA,QAAQ,EAAC;UACR,IAAI,CAACT,KAAI,GAAIS,QAAQ,CAACT,KAAK;UAC3B,IAAI,CAACI,WAAU,GAAI,IAAI,CACvBM,IAAI,CAACP,YAAW,GAAIM,QAAQ,CAACT,KAAK;QACtC,CAAC,MAAI;UACDW,OAAO,CAACC,GAAG,CAAC,0BAA0B;QAC1C;MAEJ,CAAC,CAAC;IACN,CAAC;IAEDC,WAAW,EAAE,SAAAA,CAAA,EAAU;MAEnB,IAAI,CAACX,WAAU,GAAI,IAAI,CAACA,WAAW,CAACY,IAAI,CAAC,CAAC;MAC1C,IAAI,CAACd,KAAI,GAAI,IAAI,CAACG,YAAY;MAE9B,IAAIY,WAAU,GAAI,KAAK;MACvB,IAAIC,YAAW,GAAI,CAAC,IAAI,CAACd,WAAW,CAAC;MAErC,IAAG,IAAI,CAACA,WAAW,CAACe,QAAQ,CAAC,GAAG,CAAC,EAAC;QAC9BD,YAAW,GAAI,IAAI,CAACd,WAAW,CAACY,IAAI,CAAC,CAAC,CAACI,KAAK,CAAC,GAAG,CAAC;QACjDH,WAAU,GAAIC,YAAY,CAAC,CAAC,CAAC,CAACE,KAAK,CAAC,UAAU,CAAC,CAAC,CAAC,CAAC;MACtD;MAEA,MAAMC,QAAO,GAAIH,YAAY,CAAC,CAAC,CAAC,C